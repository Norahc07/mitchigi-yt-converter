{
    "version": 2,
    "builds": [
        {
        "src": "package.json",
        "use": "@vercel/node"
        },
        {
        "src": "next.config.js",
        "use": "@vercel/next"
        }
    ],
    "env": {
        "YTDLP_PATH": "bin/yt-dlp",
        "FFMPEG_PATH": "bin/ffmpeg"
    },
    "routes": [
      {
        "src": "/(.*)",
        "dest": "/$1"
      }
    ]
  }
  